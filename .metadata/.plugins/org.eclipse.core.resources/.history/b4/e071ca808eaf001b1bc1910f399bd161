package kodlamaIO;

public class Product {
	
	public Product(int id, String name,int finishingPercent, String teacherName) {
		
		this.id=id;
		this.name=name;
		this.finishingPercent=finishingPercent;
		this.teacherName=teacherName;
	}
	
	int id;
	String name;
	int finishingPercent;
	String teacherName;
}
